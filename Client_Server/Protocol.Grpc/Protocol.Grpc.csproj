<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<!--<ImplicitUsings>enable</ImplicitUsings>-->
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Grpc" Version="2.46.6" />
		<PackageReference Include="Grpc.Tools" Version="2.54.0">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Google.Protobuf" Version="3.23.3" />	

	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Protocol\Protocol.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Protobuf Include=".\Autogenerated\Protobufs\Models.proto" GrpcServices="Client" Link="Protos\Models.proto" ProtoRoot="Autogenerated\Protobufs\" />
		<Protobuf Include=".\Autogenerated\Protobufs\PrimitiveMessages.proto" GrpcServices="Client" Link="Protos\PrimitiveMessages.proto" ProtoRoot="Autogenerated\Protobufs\" />
		<Protobuf Include=".\Autogenerated\Protobufs\ExceptionMessage.proto" GrpcServices="Client" Link="Protos\ExceptionMessage.proto" ProtoRoot="Autogenerated\Protobufs\" />
	</ItemGroup>
</Project>
